{
  "name": "narramorph-fiction",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "description": "Interactive narrative platform for creating node-based story experiences",
  "author": "Your Name",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix --fix-type layout,suggestion && prettier . --write",
    "lint:ci": "eslint . --max-warnings=0",
    "format": "prettier . --write",
    "type-check": "tsc --noEmit",
    "validate": "npm run type-check && npm run lint && npm run test",
    "content:convert": "cd tools/conversion && npm run convert",
    "content:convert:l1": "cd tools/conversion && npm run convert:l1",
    "content:convert:l2": "cd tools/conversion && npm run convert:l2",
    "content:convert:l3": "cd tools/conversion && npm run convert:l3",
    "content:convert:l4": "cd tools/conversion && npm run convert:l4",
    "content:convert:all": "cd tools/conversion && npm run convert:all",
    "content:convert:watch": "cd tools/conversion && npm run convert:watch",
    "content:validate": "cd tools/conversion && npm run validate",
    "content:matrix": "cd tools/conversion && npm run matrix",
    "content:diff": "cd tools/conversion && npm run diff",
    "content:rollback": "cd tools/conversion && npm run rollback"
  },
  "dependencies": {
    "@xyflow/react": "^12.0.0",
    "clsx": "^2.1.0",
    "framer-motion": "^11.0.0",
    "immer": "^10.0.3",
    "lucide-react": "^0.553.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^6.0.0",
    "tailwindcss": "^3.4.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^24.10.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-unused-imports": "^3.1.0",
    "jsdom": "^23.0.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.0",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{md,json,css,scss}": [
      "prettier --write"
    ]
  }
}
