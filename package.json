{
  "name": "narramorph-fiction",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "description": "Interactive narrative platform for creating node-based story experiences",
  "author": "Your Name",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "type-check": "tsc --noEmit",
    "validate": "npm run type-check && npm run lint && npm run test",
    "metadata:inventory": "node tools/inventory-l2-variations.js --root=docs",
    "metadata:inventory:report": "node tools/inventory-l2-variations.js --root=docs --output=inventory-report.json",
    "metadata:insert": "node tools/insert-l2-metadata.js --root=docs",
    "metadata:insert:batch": "node tools/insert-l2-metadata.js --root=docs --batch",
    "metadata:insert:dry": "node tools/insert-l2-metadata.js --root=docs --dry-run",
    "metadata:stub": "python tools/generate_stub_yaml.py docs --sidecar-dir .stubs/variation_headers",
    "metadata:validate": "node tools/validate-lx-metadata.js --root=docs",
    "metadata:validate:strict": "node tools/validate-lx-metadata.js --root=docs --strict",
    "metadata:autofill": "node tools/fill-l2-defaults.js --root=docs",
    "metadata:autofill:dry": "node tools/fill-l2-defaults.js --root=docs --dry-run",
    "metadata:autofill:resist": "node tools/fill-l2-defaults.js --root=docs --path=resist",
    "metadata:autofill:accept": "node tools/fill-l2-defaults.js --root=docs --path=accept",
    "metadata:autofill:invest": "node tools/fill-l2-defaults.js --root=docs --path=invest"
  },
  "dependencies": {
    "@xyflow/react": "^12.0.0",
    "clsx": "^2.1.0",
    "framer-motion": "^11.0.0",
    "immer": "^10.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^6.0.0",
    "tailwindcss": "^3.4.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^23.0.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.0",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
